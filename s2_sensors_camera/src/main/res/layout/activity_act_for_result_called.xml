<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    tools:context="com.jmarkstar.sesion2.ActForResultInitActivity">

    <Button android:text="Retornar con resultado 1"
        android:onClick="onReturnWithResult1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button android:text="Retornar con resultado 2"
        android:onClick="onReturnWithResult2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button android:text="Retornar con error"
        android:onClick="onReturnWithError"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</LinearLayout>
