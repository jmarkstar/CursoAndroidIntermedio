<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    tools:context="com.jmarkstar.sesion1.MainActivity">

    <Button android:onClick="onGoLocation"
        android:text="Ir a Gps Location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button android:onClick="onGoMaps"
        android:text="Ir a Maps"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button android:onClick="onGoMapsWithLocation"
        android:text="Ir Maps con Location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</LinearLayout>
