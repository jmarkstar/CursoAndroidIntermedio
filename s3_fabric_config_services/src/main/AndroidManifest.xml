<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jmarkstar.s3">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Sesion3Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="04afc6e3694cdef708b2b117cd776859dea9ec88" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BlockOrientationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".PreventOrientationActivity"
            android:configChanges="orientation|screenSize" />
        <activity android:name=".SaveStateActivity" />
        <activity android:name=".SaveComplexStateActivity"></activity>
    </application>

</manifest>